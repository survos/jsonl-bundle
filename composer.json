{
    "name": "survos/jsonl-bundle",
    "description": "Symfony bundle for interacting with lsonl files",
    "keywords": [
          "json", "jsonl", "nljson",
        "symfony"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.4",
        "symfony/config": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "twig/twig": "^3.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/phpunit-bridge": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/var-dumper": "^7.3",
        "symfony/http-client": "^7.3",
        "phpunit/phpunit": "^12"
    },
    "autoload": {
        "psr-4": {
            "Survos\\JsonlBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Survos\\JsonlBundle\\Tests\\": "Tests/"
        }
    },
    "suggest": {
        "survos/simple-datatables-bundle": "searchable/sortable tables when browsing"
    }
}
